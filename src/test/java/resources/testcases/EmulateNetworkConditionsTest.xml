<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Selenium.Demo">
    <listeners>
        <listener class-name="utils.ExtentReport"/>
    </listeners>
    <parameter name="browser" value="chrome"/>
    <parameter name="baseUrl" value="https://linkedin.com"/>
    <parameter name="implicitlyWait" value="10"/>
    <parameter name="pageLoadTimeout" value="100"/>
    <parameter name="printLogs" value="false"/>
    <test name="NetworkConditions">
        <classes>
            <class name="org.hadriajmnz90.EmulateNetworkConditionsTest"/>
        </classes>
    </test>

</suite>